activity {
  select {A1,B1, C1};
  A1;
  select{
    sequence {
      select{(A1) E1, F1};
      G1;
      H1;
      select {
        (E1) X1,
        Y1,
        (A1 and (B1 or C1)) Z1
      }
    };
    sequence {
      G1;
      select{M1, N1};
      H1;
      select{
       ({A1->A1} and M1) A1;
       B1;
       ({A1,G1} and N1) C1
      }
    }
  }
}
